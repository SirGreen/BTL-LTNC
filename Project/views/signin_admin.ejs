<body>
  <main>
    <section id="form" ,class="text-center">
      <h1>SIGN IN</h1>
      <!-- <% if (messages.error) { %>
            <%= messages.error %>
        <% } %> -->
      <form action="/signin_admin" method="post" id="loginForm" class="m-auto text-left" style="max-width: 500px">
        <div class="form-group">

          <label class="w-100">Account:
            <input id="Account" class="form-control" type="text" name="Account" value="" /></label>
        </div>

        <div class="form-group">
          <label class="w-100">Password:
            <input id="Password" class="form-control" type="password" name="Password" /></label>
        </div>
        <div class="form-group">
          <button id="signInButton" class="btn btn-primary" type="submit">
            Sign in
          </button>
        </div>
      </form>
      <!-- <script src="js/SubmitLogin.js"></script> -->
    </section>
  </main>
  <script type="text/javascript" src="vendor/jquery-3.5.1.min.js"></script>
  <script type="text/javascript" src="vendor/bootstrap-4.5.0-dist/js/bootstrap.min.js"></script>

  <!-- <script type="text/javascript" src="js/script.js"></script> -->
</body>

</html>