<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Đăng ký tài khoản</title>
    <link rel="stylesheet" href="vendor/bootstrap-4.5.0-dist/css/bootstrap.min.css">
    <script type="text/javascript" async=""
        src="https://www.gstatic.com/recaptcha/releases/moV1mTgQ6S91nuTnmll4Y9yf/recaptcha__en.js"
        crossorigin="anonymous"
        integrity="sha384-wEVSdqKc5hf9vkWC9kAmVRAEa11o8QNGecO6p5G2xGj+RmQhgDqFDCRdpfSr1VUf"></script>
    <script src="https://www.google.com/recaptcha/api.js" async="" defer=""></script>
    <link rel="stylesheet" href="css/style2.css">
</head>

<body>
    <main>
        <section id="form" ,class="text-center">
            <h1>SIGN IN</h1>

            <form class="m-auto text-left" style="max-width: 500px" action="/sendform_admin" method="POST">
                <div class="form-group">
                    <label class="w-100">Account:
                        <input class="form-control" type="text" name="Account" value=""></label>
                </div>

                <div class="form-group">
                    <label class="w-100">Password:
                        <input class="form-control" type="password" name="Password"></label>
                </div>
                <% if (messages.error) { %>
                    <%= messages.error %>
                        <% } %>
                            <div class="form-group">
                                <button id="signInButton" class="btn btn-primary" type="submit">
                                    Sign in
                                </button>
                            </div>
            </form>
        </section>
    </main>
    <script type="text/javascript" src="vendor/jquery-3.5.1.min.js"></script>
    <script type="text/javascript" src="vendor/bootstrap-4.5.0-dist/js/bootstrap.min.js"></script>

    <script type="text/javascript" src="js/script.js"></script>


</body>

</html>